local Tab = Window:MakeTab({
	Name = "Function",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "SKILL"
})
Tab:AddButton({
	Name = "NoCD",
	Callback = function()
		game.Players.LocalPlayer.NoCD.Value = true
	end    
})
Tab:AddButton({
	Name = "noDashCD",
	Callback = function()
		game.Players.LocalPlayer.NoDashCD.Value = true
	end    
})
Tab:AddButton({
	Name = "give Awaken",
	Callback = function()
		game.Players.LocalPlayer.AwakenBar.Value = 200
	end    
})
Tab:AddButton({
	Name = "gamepass",
	Callback = function()
		game:GetService("Players").LocalPlayer.EarlyAccess.Value = true
	end
})
Tab:AddButton({
	Name = "get kill",
	Callback = function()
		while wait() do
			local players = game:GetService("Players")
			local localPlayer = players.LocalPlayer
			for _, player in pairs(players:GetPlayers()) do
				if player ~= localPlayer then
					local args = {
						[1] = player
					}
					game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetKills"):FireServer(unpack(args))
				end
			end
		end
	end   

})
Tab:AddButton({
	Name = "kill sound",
	Callback = function()
		game:GetService("Players").LocalPlayer.KillSoundGamePass.Value = true
		if game:GetService("Players").LocalPlayer.PlayerGui.SoundKillGui.Enabled == true then
			game:GetService("Players").LocalPlayer.PlayerGui.SoundKillGui.Enabled = false
		else
			game:GetService("Players").LocalPlayer.PlayerGui.SoundKillGui.Enabled = true
		end
	end
})
Tab:AddButton({
	Name = "walkspeed",
	Callback = function()
		workspace.nurasir2011.Humanoid.WalkSpeed = 50
	end    
})
Tab:AddButton({
	Name = "Admin spacs1",
	Callback = function()
	for i = 1, 100000 do
	workspace.nurasir2011.AdminSpecs.Enabled = true
	wait(0.5)
	end
end
})
Tab:AddButton({
	Name = "Admin spacs2",
	Callback = function()
	for i = 1, 100000 do
	workspace.nurasir2011.AdminSpecs.Disabled = true
	wait(0.5)
	end
end
})
Tab:AddButton({
	Name = "keyboard",
	Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end
})

